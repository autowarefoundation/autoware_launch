<?xml version="1.0"?>
<robot name="vehicle" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:arg name="vehicle_model" default=""/>
  <xacro:arg name="sensor_model" default=""/>

  <!-- vehicle -->
  <xacro:include filename="$(find sample_vehicle_description)/urdf/vehicle.xacro"/>

  <!-- sensors -->
  <xacro:include filename="$(find sample_sensor_kit_description)/urdf/sensors.xacro"/>
</robot>
